    "version": "0.2.0",
    "configurations": [
        {
          
      "type": "node",
      "request": "launch",
      "name": "Debug Seller",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/nest",
      "runtimeArgs": ["start",  "--watch", "--debug"],
